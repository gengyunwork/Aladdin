<!--pages/applyPartner/index.wxml-->
<base-top-bar title="招募合伙人"></base-top-bar>
<view>
  <view style="width:100%;height:400rpx;">
    <image src="{{headImg}}" style="width:100%;height:100%;"></image>
  </view>
  <view class="body">权益简介</view>
  <view class="foot" wx:for="{{partnerList}}" :key>
    <view class="footTitle">
      <view style="width:40rpx;height:40rpx;margin-left:30rpx;">
        <image src="{{item.icon}}" style="width:100%;height:100%;"></image>
      </view>
      <view style="font-size:28rpx;margin-left:9rpx;">{{item.title}}</view>
    </view>
    <view style="font-size:24rpx;color:#666;margin-left:82rpx;margin-top:8rpx;padding-bottom:35rpx;padding-right:60rpx;">{{item.msg}}</view>
  </view>
  <view class="bottomBar">加入我们</view>
</view>
