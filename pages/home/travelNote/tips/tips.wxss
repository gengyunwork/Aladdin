.pipe {
  width: 3rpx;
  height: 70%;
  background-color: #e3e3e3;
}

.end-divider {
  color: purple;
  background: white;
  position: relative;
  text-align: center;
  z-index: 11;
  padding: 20rpx;
}

.end-divider::after {
  content: "";
  width: 70%;
  height: 2rpx;
  background-color: purple;
  position: absolute;
  top: 50%;
  left: 15%;
  z-index: -1;
}
