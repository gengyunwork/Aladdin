<!--pages/user/distribution/distribution.wxml-->
<!-- 分销中心 -->
<base-top-bar title="分销中心"></base-top-bar>
<view class="grid col-2 solid-bottom  padding-tb-sm bg-white">
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="flex flex-direction align-center justify-center" style="line-height:1.5;">
    <image class="dis-icon" src="../../../images/distribution/money.png"></image>
    <text>我的佣金</text>
    <text class="label">500.00元</text>
  </navigator>
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="flex flex-direction align-center justify-center">
    <image class="dis-icon" src="../../../images/distribution/card.png"></image>
    <text>佣金金额</text>
    <text class="label">500.00元</text>
    <text class="label" style="color:#666;">（冻结120.00元）</text>
  </navigator>
</view>
<view class="grid col-2 padding-tb-xs bg-white" style="border-bottom:8rpx solid #F5F5F5;">
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="flex flex-direction align-center justify-center" style="line-height:1.3;">
    <image class="dis-icon" src="../../../images/distribution/order.png"></image>
    <text>我的业绩单</text>
    <text class="label">50.00</text>
  </navigator>
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="flex flex-direction align-center justify-center">
    <image class="dis-icon" src="../../../images/distribution/team.png"></image>
    <text>我的团队</text>
    <text class="label">50人</text>
  </navigator>
</view>


<view class="cu-list  menu">
  <!-- 提现 -->
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="cu-item  menuArrow">
    <view class="content">
      <image class="dis-icon--s" src="../../../images/distribution/walate.png"></image>
      <text class="text-grey">提现</text>
    </view>
    <view class="action">
      <text class="cuIcon-right"></text>
    </view>
  </navigator>
  <!-- 提现记录 -->
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="cu-item  menuArrow" style="border-bottom:8rpx solid #F5F5F5;">
    <view class="content">
      <image class="dis-icon--s" src="../../../images/distribution/order2.png"></image>
      <text class="text-grey">提现记录</text>
    </view>
    <view class="action">
      <text class="cuIcon-right"></text>
    </view>
  </navigator>
  <!-- 我的团队 -->
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="cu-item  menuArrow">
    <view class="content">
      <image class="dis-icon--s" src="../../../images/distribution/team2.png"></image>
      <text class="text-grey">我的团队</text>
    </view>
    <view class="action">
      <text class="cuIcon-right"></text>
    </view>
  </navigator>
  <!-- 邀请分销成员 -->
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="cu-item  menuArrow">
    <view class="content">
      <image class="dis-icon--s" src="../../../images/distribution/pie.png"></image>
      <text class="text-grey">邀请分销成员</text>
    </view>
    <view class="action">
      <text class="cuIcon-right"></text>
    </view>
  </navigator>
  <!-- 分销说明 -->
  <navigator url="/pages/user/distribution/brokerage/brokerage" class="cu-item  menuArrow">
    <view class="content">
      <image class="dis-icon--s" src="../../../images/distribution/help.png"></image>
      <text class="text-grey">分销说明</text>
    </view>
    <view class="action">
      <text class="cuIcon-right"></text>
    </view>
  </navigator>
</view>