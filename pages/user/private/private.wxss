/* pages/user/private/private.wxss */

.listShow {
  width: 100%;
  height: 100rpx;
  background: white;
  border-bottom: 1rpx solid #f5f5f5;
  padding: 1rpx 30rpx;
  padding-right: 25rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}

.listName {
  font-size: 30rpx;
}

.listOthers ,.cuIcon-right {
  font-size: 28rpx;
  color: #666;
}
.cuIcon-right {
  font-size: 35rpx;
  margin-left: 15rpx;
}
.cu-form-group picker .picker {
  color: #666;
}

